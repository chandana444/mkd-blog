    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "marked": "^4.0.8",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nanoid": "^3.1.30",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.2",
    "otp-generator": "^4.0.0",
    "rebrandly": "^2.0.1",
    "sequelize": "^6.12.5",
    "turndown": "^7.1.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "dotenv": "^10.0.0",
    "node-sass": "^7.0.1",
